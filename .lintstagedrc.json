{
  "*.{ts,tsx}": [
    "eslint --fix",
    "prettier --write",
    "jest --bail --findRelatedTests --passWithNoTests"
  ],
  "*.{js,jsx}": ["eslint --fix", "prettier --write"],
  "*.{json,css,md}": ["prettier --write"],
  "*.config.js": ["eslint --fix", "prettier --write"],
  "test-flags.js": ["eslint --fix", "prettier --write"]
}
